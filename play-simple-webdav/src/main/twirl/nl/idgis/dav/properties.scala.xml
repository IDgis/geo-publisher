@(path: String, properties: Optional[nl.idgis.dav.model.ResourceProperties])<D:multistatus xmlns:D="DAV:">
	<D:response>
		<D:href>@path</D:href>
		<D:propstat>
			@if(properties.isPresent()) {
				@propertiesContent(properties.get())
				<D:status>HTTP/1.1 200 OK</D:status>
			} else {
				<D:status>HTTP/1.1 404 Not Found</D:status>
			}
		</D:propstat>
	</D:response>
</D:multistatus>