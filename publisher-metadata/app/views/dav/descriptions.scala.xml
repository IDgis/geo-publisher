@(path: String, descriptions: List[model.dav.ResourceDescription])<D:multistatus xmlns:D="DAV:">
	@for(description <- descriptions) {
		<D:response>
			<D:href>@path@description.name()</D:href>
			<D:propstat>
				@propertiesContent(description.properties)
				<D:status>HTTP/1.1 200 OK</D:status>
			</D:propstat>
		</D:response>
	}	
</D:multistatus>